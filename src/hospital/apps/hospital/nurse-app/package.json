{
  "name": "@ducen/hospital-nurse-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:test",
    "build:test": "npm run build -w @ducen/hospital -w @ducen/shared -w @ducen/ui-native",
    "start": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build --profile preview --platform android --non-interactive --no-wait",
    "build:local": "timestamp=$(date \"+%F_%T\") && eas build --profile preview --platform android --local --output=./build/attention-room-$timestamp.apk",
    "test": "jest",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "dependencies": {
